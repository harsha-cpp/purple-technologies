{
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "framework": "qwik",
    "functions": {
      "src/**/*.ts": {
        "runtime": "@vercel/node"
      }
    },
    "rewrites": [
      {
        "source": "/((?!api/).*)",
        "destination": "/server/entry.vercel.js"
      }
    ]
  }